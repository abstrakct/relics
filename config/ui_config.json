{
  "keybindings": {
    "Menu": {
      "<esc>": "Quit",
      "<j>": "NextMenuItem",
      "<k>": "PrevMenuItem",
      "<down>": "NextMenuItem",
      "<up>": "PrevMenuItem",
      "<enter>": "SelectMenuItem"
    },
    "InGame": {
      "<esc>": "ShowMainMenu",
      "<j>": { "PlayerMoveRelative": { "dx": 0, "dy": 1 } },
      "<k>": { "PlayerMoveRelative": { "dx": 0, "dy": -1 } },
      "<h>": { "PlayerMoveRelative": { "dx": -1, "dy": 0 } },
      "<l>": { "PlayerMoveRelative": { "dx": 1, "dy": 0 } },
      "<y>": { "PlayerMoveRelative": { "dx": -1, "dy": -1 } },
      "<u>": { "PlayerMoveRelative": { "dx": 1, "dy": -1 } },
      "<b>": { "PlayerMoveRelative": { "dx": -1, "dy": 1 } },
      "<n>": { "PlayerMoveRelative": { "dx": 1, "dy": 1 } },
      "<q>": "Quit"
    }
  }
}
